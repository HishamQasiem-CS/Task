@model IEnumerable<Menu>
@{
    Layout = null;
}
<title>Menu Information</title>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">


<table class="table table-striped" style="text-align:center;  margin-top:10px;">
    <thead>
        <tr>
            <th scope="col">@Html.DisplayNameFor(x=>x.MenuId)</th>
            <th scope="col">@Html.DisplayNameFor(x=>x.ItemName)</th>
            <th scope="col">@Html.DisplayNameFor(x=>x.SavingTime)</th>
         

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MenuId</td>
                <td>@item.ItemName</td>
                <td>@item.SavingTime</td>
               
            </tr>
        }
    </tbody>
</table>

<a asp-action="All_Information" asp-controller="Home" class="btn btn-primary">Back to Home Page</a>